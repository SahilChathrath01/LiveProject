{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "empty-table-header": "off",
        "aria-required-parent": "off",
        "aria-input-field-name": "off",
        "aria-required-children": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ]
  }
}